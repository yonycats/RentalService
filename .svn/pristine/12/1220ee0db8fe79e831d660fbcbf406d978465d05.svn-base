package kr.or.ddit.vo;

public class CardVO {

	private String cardId;
	private String cardCom;
	private String cardNo;
	private String cardDate;
	private String cardDisc;
	private String userNo;
	
	public CardVO() {
	}

	public CardVO(String cardCom, String cardNo, String cardDate, String userNo) {
		super();
		this.cardCom = cardCom;
		this.cardNo = cardNo;
		this.cardDate = cardDate;
		this.userNo = userNo;
	}


	public String getUserNo() {
		return userNo;
	}

	public void setUserNo(String userNo) {
		this.userNo = userNo;
	}

	public String getCardId() {
		return cardId;
	}

	public void setCardId(String cardId) {
		this.cardId = cardId;
	}

	public String getCardCom() {
		return cardCom;
	}

	public void setCardCom(String cardCom) {
		this.cardCom = cardCom;
	}

	public String getCardNo() {
		return cardNo;
	}

	public void setCardNo(String cardNo) {
		this.cardNo = cardNo;
	}

	public String getCardDate() {
		return cardDate;
	}

	public void setCardDate(String cardDate) {
		this.cardDate = cardDate;
	}

	public String getCardDisc() {
		return cardDisc;
	}

	public void setCardDisc(String cardDisc) {
		this.cardDisc = cardDisc;
	}

	@Override
	public String toString() {
		return "CardVO [cardId=" + cardId + ", cardCom=" + cardCom + ", cardNo=" + cardNo + ", cardDate=" + cardDate
				+ ", cardDisc=" + cardDisc + ", userNo=" + userNo + "]";
	}


	
}
