.carousel-inner img {
	width: 100%;
	margin: auto;
	min-height: 200px;
}

@media ( max-width : 600px) {
	.carousel-caption {
		display: none;
	}
}

a {
	text-decoration: none;
	color: black;
}

#header-category {
	display: flex;
	list-style: none;
	justify-content: center;
	font-size: 20px;
}

#header-category li {
	color: black;
	padding: 8px 12px;
}

#header-category li::after {
	content: "|";
	padding-left: 24px;
}

#header-category li:last-child:after {
	content: "";
}

body {
	overflow-y: auto;
}

.search-box {
	height: 30px;
	background: white;
	border-radius: 40px;
	padding-left: 10px;
	border: 1px solid black;
	margin-bottom: 10px;
}

.search-txt {
	width: auto;
	border: none;
	background: none;
	outline: none;
	float: left;
	padding-top: 1px;
	color: black;
	font-size: 15px;
	line-height: 30px;
	align-items: center;
	border-radius: 10px;
	margin: 4px 0 0 5px;
}

.search-btn {
	float: right;
	margin-right: 10px;
	width: 40px;
	height: 100%;
	border-radius: 50%;
	background: white;
	border: none;
	font-size: 18px;
	display: flex;
	justify-content: center;
	align-items: center;
}

.search-box-container {
	display: flex;
	justify-content: flex-end;
	align-items: center;
	height: 30px;
}

.reset-btn {
	background-color: #e2e2e2;
	border: 1px solid black;
	color: black;
	padding: 5px 0px;
	border-radius: 5px;
	cursor: pointer;
	font-size: 13px;
	width: 60px;
}

.reset-btn:hover {
	background-color: #ccc;
}

.product-item .rental {
	margin-inline-start: 10px;
}

.product-item .rental-fee {
	margin: 0px;
	float: left;
	font-size: 17px;
	font-weight: 500;
}

.product-item .rental-amount {
	margin: 0px;
	float: right;
	font-size: 20px;
	font-weight: 600;
}

.product-item .rental-amount-won {
	margin: 0px;
	font-size: 16px;
	font-weight: 400;
	margin-left: 2px;
}

.product-item .modelName {
	color: gray;
	text-align: start;
	padding-top: 3px;
	margin-bottom: 0;
	font-size: 12px;
	margin-inline-start: 10px;
}

.product-item .productName {
	text-align: start;
	margin-bottom: 10px;
	font-size: 14px;
	font-weight: 400;
	margin-inline-start: 10px;
}

.product-item img {
	height: 100% px;
	width: 100% px;
	object-fit: cover;
	margin-bottom: 10px;
}

.product-item {
	border-radius: 20px;
	border: 1px solid #ccc;
	transition: border-color 0.5s ease;
	margin-bottom: 10px;
	text-align: center;
	padding: 20px;
	display: flex;
	flex-direction: column;
	justify-content: center;
	margin: 10px;
	width: 270px;
	height: 430px;
}

.product-item:hover {
	border: 1px solid black;
}

.fixed-widget {
	position: fixed;
	right: 30px;
	width: 60px;
	padding: 15px;
	display: flex;
	flex-direction: column;
	align-items: center;
	gap: 20px;
	z-index: 1300;
}

.fixed-widget a {
	width: 60px;
	height: 60px;
	border-radius: 30px;
	display: flex;
	align-items: center;
	justify-content: center;
	background-color: white;
}

.fixed-widget img {
	width: 60px;
	height: 60px;
	border-radius: 30px;
}

.navbar-nav>.separator {
	display: flex;
	align-items: center;
	padding-left: 10px;
	padding-right: 10px;
	color: black;
	height: 50px;
	pointer-events: none;
}

#widget1 {
	background-color: #fae21c;
}

#wrap {
	min-height: 100vh;
	position: relative;
	width: 100%;
	display: flex;
	flex-direction: column;
}

footer {
	background-color: #232323;
	width: 100%;
	height: 220px;
	bottom: 0px;
	position: relative;
	color: #dddddd;
}

.footer-info {
	float: left;
}

#footer-category {
	display: flex;
	list-style: none;
	justify-content: start;
	font-size: 15px;
	font-weight: 400;
}

#footer-category a {
	color: white;
}

#footer-category li {
	color: #dddddd;
	padding: 8px 12px;
}

#footer-category li::after {
	content: "|";
	padding-left: 24px;
}

#footer-category li:last-child:after {
	content: "";
}

#footer-info {
	display: flex;
	list-style: none;
	flex-direction: column;
	padding: 10px 0px 0px 60px;
}

#footer-info li {
	margin-bottom: 5px;
}

.filter {
	background-color: #f7f7f7;
	left: 0;
	z-index: 1;
	width: 20%;
	padding-top: 10px;
}

.filter-box-container {
	padding: 5px;
	border-radius: 5px;
	margin: 0px 10px 0px 10px;
}

.filter-header {
	display: flex;
	justify-content: space-between;
	align-items: center;
	margin-bottom: 20px;
}

.filter-header h2 {
	margin: 0;
	font-size: 24px;
}

.filter-category {
	display: flex;
	flex-direction: column;
	align-items: center;
}

.filter-category-name {
	font-size: 14px;
	font-weight: bold;
	margin-bottom: 3px;
}

.filter-list {
	list-style-type: none;
	display: flex;
	flex-wrap: wrap;
	gap: 10px;
	justify-content: space-between;
	width: 100%;
	border: 1px solid black;
	padding: 10px;
	border-radius: 10px;
	margin-bottom: 20px;
}

.filter-list li {
	width: 100%;
	height: 100%;
	display: flex;
	align-items: center;
	font-size: 15px;
}

.filter-list li label {
	align-items: center;
}

.filter-list input[type="checkbox"] {
	margin-right: 10px;
}

.col-sm-8 {
	display: flex;
	flex-direction: column;
}

.category-header {
	text-align: center;
	padding: 20px 0;
	border-bottom: 1px solid #ccc;
}

.sort-and-count {
	display: flex;
	justify-content: space-between;
	align-items: center;
	padding: 10px 0;
	border-bottom: 1px solid #ccc;
}

.product-count {
	font-size: 18px;
}

.sort-options {
	display: flex;
	gap: 10px;
}

.sort-btn {
	background-color: white;
	border: none;
	color: black;
	padding: 5px 10px;
	border-radius: 5px;
	cursor: pointer;
}

.sort-btn:hover {
	background-color: #ccc;
}

.search-sort-btn {
	background-color: white;
	border: none;
	color: black;
	padding: 5px 10px;
	border-radius: 5px;
	cursor: pointer;
}

.search-sort-btn:hover {
	background-color: #ccc;
}

.product-list {
	display: flex;
	flex-direction: row;
	flex-wrap: wrap;
	justify-content: space-evenly;
	margin: 0px 20px;
	width: 84%
}

.product-item {
	box-sizing: border-box;
	margin-bottom: 20px;
}

.detail-selectable-button {
	margin: 10px 0;
	padding: 10px 20px;
	border: 2px solid #aaa;
	border-radius: 10px;
	background-color: white;
	cursor: pointer;
}

.detail-selectable-button.selected {
	border-color: red;
}

.detail-select-ul {
	display: flex;
	justify-content: center;
	list-style: none;
	padding: 0;
	margin: 0;
}

.detail-selectable-button {
	font-size: 15px;
	font-weight: 400;
	margin-right: 30px;
	border-radius: 10px;
}

.vi_price_bx {
	padding: 20px 25px;
	background-color: #f7f7f7;
	height: 80px;
}

.vi_price_bx dl {
	width: 100%;
	line-height: 1.4em;
}

.vi_price_bx dl.priceBx3 dt {
	font-size: 16px;
	font-weight: 500;
	color: #111;
	padding: 7px 0 0;
}

.vi_price_bx dl dt {
	width: 40%;
	font-size: 14px;
	color: #888;
	text-align: left;
}

.vi_price_bx dl dt, .vi_price_bx dl dd {
	padding: 5px 0;
	float: left;
}

.vi_price_bx dl.priceBx3 dd {
	font-size: 24px;
	font-weight: 800;
	color: #c60101;
}

.vi_price_bx dl dd {
	width: 60%;
	font-size: 18px;
	color: #333;
	font-weight: 600;
	text-align: right;
}

.vi_price_bx dl dd {
	padding: 5px 0;
	float: left;
}

.vi_price_bx dl.priceBx3 dd span {
	font-size: 20px;
}

.button-container {
	display: flex;
	justify-content: space-between;
	align-content: center;
	padding: 10px;
	height: 100px;
}

.button-container button {
	margin-top: 30px;
	padding: 10px 20px;
	border: 2px solid transparent;
	background-color: #0e2e3d;
	cursor: pointer;
	width: 45%;
	height: 50px;
	border: 1px solid black;
	color: white;
	font-size: 15px;
	font-weight: 500;
	border-radius: 10px;
}

.button-container button:hover {
	background-color: #d0eaff;
	color: black;
}

.item-strPrice {
	font-weight: bold;
	font-size: 18px;
}

.compare {
	margin-top: 90px;
}

.compare h2 {
	font-size: 25px;
	font-weight: 500;
	color: #333;
	letter-spacing: -1px;
}

.compare h2 .stxt {
	font-size: 15px;
	color: #888;
	font-weight: 400;
	margin: 0 0 0 10px;
}

.compare .compare_tbl {
	width: 100%;
	border-top: 1px solid #666;
	margin-top: 12px;
}

colgroup {
	display: table-column-group;
	unicode-bidi: isolate;
}

tr {
	display: table-row;
	vertical-align: inherit;
	unicode-bidi: isolate;
	border-color: inherit;
}

.compare .compare_tbl thead th {
	font-size: 16px;
	font-weight: 500;
	color: #333;
	padding: 17px 0;
}

table th {
	background-color: #f8f8f8;
	white-space: nowrap;
	text-overflow: ellipsis;
	line-height: 1.3em;
}

tbody {
	display: table-row-group;
	vertical-align: middle;
	unicode-bidi: isolate;
	border-color: inherit;
}

.compare .compare_tbl tbody td.brname img {
	max-width: 160px;
	width: auto;
	max-height: 55px;
}

label, input, button, textarea, select, img {
	vertical-align: middle;
}

img, table, thead, tfoot, fieldset, legend, iframe {
	border: none;
}

img {
	overflow-clip-margin: content-box;
	overflow: clip;
}

.compare .compare_tbl tbody td {
	padding: 15px 12px;
	text-align: center;
	border-bottom: 1px solid #e1e1e1;
	vertical-align: middle;
}

.compare .compare_tbl tbody td.brname p {
	font-size: 14px;
	margin-top: 5px;
}

.compare .compare_tbl tbody td.comp_price dl {
	width: 100px;
	padding: 12px 0;
	border: 2px solid #ccc;
	margin: 0 30px;
	border-radius: 5px;
	display: inline-block;
	vertical-align: middle;
}

.compare .compare_tbl tbody td.comp_price dl dt {
	font-size: 14px;
	text-align: center;
}

.compare .compare_tbl tbody td.comp_price dl dd {
	font-size: 16px;
	font-weight: 500;
	text-align: center;
	margin-top: 2px;
}

b, strong {
	font-weight: 600;
}

.vi_tab ul li.active {
	color: #0e2f3c;
}

.vi_tab li.active::before {
	height: 3px;
	background: #0e2f3c;
	display: block;
	content: '';
	position: absolute;
	bottom: -1px;
	left: 0;
	right: 0;
	z-index: 10;
}

.vi_tab li {
	width: 33%;
	height: 50px;
	color: #888;
	font-size: 17px;
	font-weight: 500;
	line-height: 50px;
	text-align: center;
	overflow: visible;
	border-bottom: 1px solid #e1e1e1;
	cursor: pointer;
	float: left;
	position: relative;
}

.mart80 {
	margin-top: 80px !important;
}

.pr {
	position: relative;
}

.via {
	position: absolute;
	top: -48px;
	left: 0;
}

.vi_cbody {
	margin-top: 40px;
}

#product-item-list {
	margin-left: 232px;
}