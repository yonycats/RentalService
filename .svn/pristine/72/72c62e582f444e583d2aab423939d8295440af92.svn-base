package kr.or.ddit.vo;

public class ItemInquiryVO {

	private String inqId;
	private String inqTitle;
	private String inqContent;
	private String inqDate;
	private String inqImage;
	
	private String userNo;
	private String itemId;
	
	public ItemInquiryVO() {
		
	}

	
	
	
	public ItemInquiryVO(String inqId, String inqTitle, String inqContent, String inqImage) {
		super();
		this.inqId = inqId;
		this.inqTitle = inqTitle;
		this.inqContent = inqContent;
		this.inqImage = inqImage;
	}




	public ItemInquiryVO(String inqTitle, String inqContent, String inqImage, String userNo,
			String itemId) {
		super();
		this.inqTitle = inqTitle;
		this.inqContent = inqContent;
		this.inqImage = inqImage;
		this.userNo = userNo;
		this.itemId = itemId;
	}
	
	

	public String getInqId() {
		return inqId;
	}

	public void setInqId(String inqId) {
		this.inqId = inqId;
	}

	public String getInqTitle() {
		return inqTitle;
	}

	public void setInqTitle(String inqTitle) {
		this.inqTitle = inqTitle;
	}

	public String getInqContent() {
		return inqContent;
	}

	public void setInqContent(String inqContent) {
		this.inqContent = inqContent;
	}

	public String getInqDate() {
		return inqDate;
	}

	public void setInqDate(String inqDate) {
		this.inqDate = inqDate;
	}

	public String getInqImage() {
		return inqImage;
	}

	public void setInqImage(String inqImage) {
		this.inqImage = inqImage;
	}

	public String getUserNo() {
		return userNo;
	}

	public void setUserNo(String userNo) {
		this.userNo = userNo;
	}

	public String getItemId() {
		return itemId;
	}

	public void setItemId(String itemId) {
		this.itemId = itemId;
	}

	@Override
	public String toString() {
		return "ItemInquiryVO [inqId=" + inqId + ", inqTitle=" + inqTitle + ", inqContent=" + inqContent + ", inqDate="
				+ inqDate + ", inqImage=" + inqImage + ", userNo=" + userNo + ", itemId=" + itemId + "]";
	}
	
	
	
	
}
