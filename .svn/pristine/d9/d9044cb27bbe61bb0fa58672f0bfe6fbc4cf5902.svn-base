@charset "UTF-8";

form {
	margin: 0;
	padding: 0;
	display: block;
}

#fregisterform h3 {
	margin: 20px 0 5px;
}

.tbl_frm01 {
	border-top: 1px solid #666;
}

#radio_select_form {
	border-top: 1px solid #666;
}

.tbl_frm01 table {
	width: 100%;
}

#radio_select_form {
	border-top: 1px solid #666;
}

table {
	border-collapse: collapse;
	border-spacing: 0;
	border: none;
}

colgroup {
	display: table-column-group;
}

.w140 {
	width: 140px;
}

col {
	display: table-column;
}

tbody {
	display: table-row-group;
	vertical-align: middle;
}

tr {
	display: table-row;
	vertical-align: inherit;
}

.tbl_frm01 th, .tbl_frm02 th, .tbl_head01 th, .tbl_head02 th {
	font-weight: 500;
}

.tbl_frm01 {
	font-size: 14px;
}

.tbl_frm01 th, .tbl_frm01 td {
	padding: 12px 20px;
	min-height: 30px;
	border-bottom: 1px solid #e1e1e1;
	text-align: left;
	vertical-align: middle;
}

table th {
	background-color: #f8f8f8;
	white-space: nowrap;
	text-overflow: ellipsis;
	line-height: 1.3em;
}

label {
	vertical-align: middle;
	cursor: default;
	font-size: 14px;
	margin: 0;
}

.tbl_frm01 td {
	font-size: 14px;
}

.frm_input {
	padding: 0 10px;
	font-weight: normal;
	border: 1px solid #e1e1e1;
	height: 30px;
}

.frm_info {
	display: block;
	padding-top: 5px;
	line-height: 1.4em;
}

#fregisterform h3 {
	margin: 20px 0 5px;
}

h3 {
	font-size: 1em;
	font-weight: 600;
	line-height: 1.1;
}

.sound_only {
	display: inline-block;
	position: absolute;
	top: 0;
	left: 0;
	margin: 0;
	padding: 0;
	width: 1px;
	height: 1px;
	font-size: 0;
	line-height: 0;
}

.btn_lsmall {
	background: #333;
	border: 1px solid #333;
	color: #fff;
	padding: 10px 12px;
	font-size: 14px;
	font-weight: normal;
	display: inline-block;
	text-decoration: none;
	vertical-align: middle;
	font-family: inherit;
	line-height: 1em;
	white-space: nowrap;
	cursor: pointer;
}

.tbl_frm01 .frm_address {
	margin-top: 5px;
}

.btn_confirm {
	margin-top: 47px;
	text-align: center;
}

.btn_confirm input {
	margin: 0 1.5px;
}

.wset {
	background: #0e2f3c;
	border-color: #0e2f3c;
	color: #fff;
}

.btn_signUp {
	display: flex;
	align-items: center;
	justify-content: center; 
/* 	position: absolute; */
/* 	left: 50%; */
/* 	transform: translateX(-50%); */
}

.btn_large {
	font-weight: 500;
	padding: 18px 25px;
	font-size: 17px;
}

.btn_confirm a {
	margin: 0 1.5px;
}

.bx-white {
	background-color: #fff;
	border: 1px solid #ccc;
	color: #222;
}

#captcha_img {
	width: 300px;
	height: 200px;
}

#captcha_box {
	display: flex;
	flex-direction: row;
}

#captcha-box-right {
	background-color: #fff;
	border-radius: 8px;
	padding: 20px;
	max-width: 300px;
}

#captcha-box-right p {
	font-size: 14px;
	color: #333;
	line-height: 1.6;
	margin-bottom: 20px;
}

#captcha_text {
	display: flex;
	justify-content: space-between;
	align-items: center;
}

#captcha::placeholder {
	color: #999;
}

#captcha {
	width: 60%;
	padding: 10px;
	border: 1px solid #ccc;
	border-radius: 4px;
	font-size: 14px;
}

.btn {
	background-color: #0e2f3c;
	color: white;
	border: none;
	padding: 10px 15px;
	border-radius: 4px;
	font-size: 14px;
	cursor: pointer;
	transition: background-color 0.3s;
}

#h1 {
	padding-top: 10px;
}

#radio_select_form {
	padding-bottom: 10px;
}

.rq {
	color: red;
}

.sign-table td: not(last-child) {
	text-align: center;
	width: 50px;
}